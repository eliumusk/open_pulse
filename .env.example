# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenRouter (alternative)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# MCP Tools API Keys
BRAVE_API_KEY=your_brave_api_key_here

# Custom Readers API Keys
JINA_API_KEY=your_jina_api_key_here  # Get free at https://jina.ai/reader

# Database
DATABASE_URL=sqlite:///./open_pulse.db

# AgentOS Configuration
AGENTOS_PORT=7777
AGENTOS_HOST=0.0.0.0

# Scheduler Configuration
NEWSLETTER_GENERATION_HOUR=8
NEWSLETTER_GENERATION_MINUTE=0

# Scheduler Mode: development, production, or once
# - development: Fast intervals for testing (every 2 minutes)
# - production: Daily at specified time
# - once: Run once after delay (for testing)
SCHEDULER_MODE=development

# Telemetry (optional)
AGNO_TELEMETRY=false

COHERE_API_KEY=xxxx

FIRECRAWL_API_KEY = xxxx

GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here
GOOGLE_PROJECT_ID=your_project_id_here
GOOGLE_REDIRECT_URI=http://localhost  # Default value